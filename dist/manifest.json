{
  "manifest_version": 3,
  "name": "LeetAssist AI",
  "version": "1.0.0",
  "description": "Chrome extension providing AI-driven hints on LeetCode problems. Get step-by-step help to boost problem-solving skills effectively.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.tsx-loader-MIS3qm3q.js"
      ],
      "matches": [
        "https://leetcode.com/problems/*"
      ],
      "css": [
        "assets/select-QPmKmYvM.css"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://leetcode.com/*"
      ],
      "resources": [
        "assets/select-BWekHH8c.js",
        "assets/content.tsx-u0iwHVBa.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
